<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <!--theme style-->
    @Styles.Render("~/Content/style.css")
    @Styles.Render("~/Content/bootstrap.css")

    @Scripts.Render("~/Scripts/jquery-1.10.2.js")
    <!--//theme style-->
    <!-- Custom Theme files -->
    @Scripts.Render("~/Scripts/bootstrap.js")

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />

    <!-- cart menu -->
    @*@Scripts.Render("~/Scripts/simpleCart.min.js")*@
    <!--//cart menu -->
    <!-- start menu -->
    @Styles.Render("~/Content/memenu.css")
    <!--//start menu -->

    @Scripts.Render("~/Scripts/main.js")
</head>
<body>
    @Html.Partial("../Partial/AdminHeader")

    @RenderBody()

    @Html.Partial("../Partial/AdminFooter")
    <div class="copywrite">
        <div class="container">
            <div class="copy">
                <p>
                    © 2015 Lighting. All Rights Reserved | Design by <a href="http://w3layouts.com/" target="_blank">W3layouts</a>
                </p>
            </div>
            <div class="social">
                <a href="#"><i class="facebook"></i></a>
                <a href="#"><i class="twitter"></i></a>
                <a href="#"><i class="dribble"></i></a>
                <a href="#"><i class="google"></i></a>
                <a href="#"><i class="youtube"></i></a>
            </div>
            <div class="clearfix"></div>
        </div>
    </div>
</body>
</html>